{
  "private": true,
  "author": "husongtao <15309487104@163.com>",
  "scripts": {
    "build": "umi build",
    "dev": "umi dev",
    "postinstall": "umi setup",
    "setup": "umi setup",
    "start": "umi dev",
    "lint": "umi g tmp && pnpm lint:js && pnpm lint:style && pnpm lint:prettier && pnpm tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src &&pnpm lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "test:coverage": "umi-test --coverage"
  },
  "githooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e  $GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,less}": [
      "umi lint --fix"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@ant-design/pro-components": "^2.7.10",
    "@types/lodash.clonedeep": "^4.5.9",
    "echarts-for-react": "^3.0.2",
    "table-render": "^2.1.4"
  },
  "devDependencies": {
    "@ant-design/plots": "^2.2.5",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@umijs/fabric": "^4.0.1",
    "@umijs/plugins": "^4.3.1",
    "ant-design-pro": "^2.3.2",
    "antd": "^5.19.1",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "echarts": "^5.5.1",
    "lint-staged": "^15.2.7",
    "rc-resize-observer": "^1.4.0",
    "tailwindcss": "^3",
    "typescript": "^5.0.3",
    "umi": "^4.3.1",
    "umi-request": "^1.4.0"
  }
}
